{
  "name": "root",
  "private": true,
  "scripts": {
    "test": "npx jest --maxWorkers=2",
    "build": "tsc -b",
    "build:all": "nx run-many --target=build --all",
    "build:since": "nx affected --target=build",
    "clean-build": "npm run clean && npm run build",
    "update-readme-packages": "ts-node scripts/packages.ts update",
